<Window x:Class="Equipment_Accounting.View.SelectContract"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Equipment_Accounting.View"
        mc:Ignorable="d"
        Title="Тип заявки" WindowStartupLocation="CenterScreen"  Height="150" Width="400" MaxHeight="150" MaxWidth="400">
    <StackPanel>
        <ComboBox materialDesign:HintAssist.Hint="Тип" Style="{StaticResource MaterialDesignOutlinedComboBox}" Margin="10" DisplayMemberPath="Name" SelectedValuePath="ID" x:Name="typeC"></ComboBox>
        <Button Margin="10" x:Name="select" Click="select_Click">Выбрать</Button>
    </StackPanel>
</Window>
