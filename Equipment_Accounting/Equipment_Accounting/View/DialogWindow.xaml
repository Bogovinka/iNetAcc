<Window x:Class="Equipment_Accounting.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Equipment_Accounting"
        mc:Ignorable="d"
        Title="DialogWindow" MaxHeight="200" MaxWidth="300" MinHeight="200" MinWidth="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="txt" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="12">Вы уверенны что хотите</Label>
        <Button x:Name="no" VerticalAlignment="Bottom" HorizontalAlignment="Left" Click="no_Click">Нет</Button>
        <Button x:Name="yes" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="yes_Click">Да</Button>
    </Grid>
</Window>
