<Window x:Class="Equipment_Accounting.View.AddSize"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Equipment_Accounting.View"
        mc:Ignorable="d"
        Title="Количество" WindowStartupLocation="CenterScreen"  Height="150" Width="400" MaxHeight="150" MaxWidth="400">
    <StackPanel>
        <TextBox materialDesign:HintAssist.Hint="Количество" Style="{StaticResource MaterialDesignOutlinedTextBox}" Margin="10" x:Name="text"></TextBox>
        <Button Margin="10" x:Name="select" Click="select_Click">ОК</Button>
    </StackPanel>
</Window>

